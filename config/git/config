[alias]
	co = checkout
	c = commit
	st = status
	b = branch
	sm = submodule
	st = status -sb --ignore-submodules=dirty
	dc = diff --cached
	ds = !git --no-pager diff --stat
	conflicts = diff --name-only --diff-filter=U
	clean-merged = !git-clean-merged
	staged = diff --cached --stat
	log1 = log --pretty='%C(auto)%h%Creset %<(12,trunc)%aN %s'

[merge]
	tool = vimdiff

[push]
	default = simple

[rerere]
	enabled = true

[include]
	path = local
	path = parameters

# vim: noexpandtab
